<%= link_to "Back to All Bands", bands_url %>

<h1>Edit <%= @band.name %> </h1>

<form action=" <%= band_url %> " method="post">
  <input type="hidden" name="_method" value="PATCH">
  <%= auth_token %>
  <label for="band_name">Name</label>
  <input type="text" name="band[name]" value=" <%= @band.name %>">
  <br>
  <br>
  <input type="submit" name="submit" value="Submit">
</form>
